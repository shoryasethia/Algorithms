    "import numpy as np\n",
    "\n",
    "# Example matrix\n",
    "A = np.array([[1, 2],\n",
    "              [3, 4]])\n",
    "\n",
    "# Compute the inverse\n",
    "A_inv = np.linalg.inv(A)\n",
    "\n",
    "print(\"Original matrix:\")\n",
    "print(A)\n",
    "\n",
    "print(\"\\nInverse matrix:\")\n",
    "print(A_inv)\n"
   ]
  }
